<div>
	<div>
		<table>
			<thead>
				<tr>
					<h1>Books</h1>
					<th>title</th>
					<th>body</th>
					<th></th>
					<th colspan="3"></th>
				</tr>
			</thead>
			<tbody>
				<% @books.each do |book|%>
					<tr>
						<th><%= book.title%></th>
						<th><%= book.body%></th>
						<th>show</th>
						<th>edit</th>
						<th>delite</th>
					</tr>
				<%end%>
			</tbody>
		</table>
	</div>
	<div>
		<h3 class="headline">New book</h3>
		<%= form_for(@book) do |f| %>
		<h4>title</h4>
		<%= f.text_field :title %>
		<h4>body</h4>
		<%= f.text_area :body %>
        <%= f.submit "Create Book" %>

<%end%>
	</div>

</div>
